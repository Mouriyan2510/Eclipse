package com.one.day1;

import java.util.Arrays;

public class CheckTwoArray {
	    public static boolean areArraysEqual(int[] array1, int[] array2) {
	        // First, check if the arrays have the same length
	        if (array1.length != array2.length) {
	            return false;
	        }

	        // Iterate through each element of the arrays and compare them
	        for (int i = 0; i < array1.length; i++) {
	            if (array1[i] != array2[i]) {
	                return false; // If any pair of elements is not equal, arrays are not equal
	            }
	        }

	        // If the loop finishes without finding any unequal elements, arrays are equal
	        return true;
	    }

	    public static void main(String[] args) {
	        int[] array1 = {1, 2, 1, 4, 5};
	        int[] array2 = {1, 2, 1, 4, 5};
	        boolean result = areArraysEqual(array1, array2);
//	        System.out.println("Are arrays equal? " + result); // Output: Are arrays equal? true
	        
	        if(Arrays.equals(array1, array2)) {
	        	System.out.println("Same");
	        }
	        else {
	        	System.out.println("Not");
	        }
	    }
	    
	    
	}
