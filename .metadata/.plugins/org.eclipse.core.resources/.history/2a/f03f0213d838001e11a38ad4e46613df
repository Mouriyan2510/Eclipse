package Basic;

public class Factorial {
	
	public int fact(int n) {
		if(n<=1) {
			return 1;
		}
		else {
			return fact(n-1)+fact(n-2);
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Factorial f1 = new Factorial();
		System.out.println(f1.fact(5));
		
		
	}

}
