package com.one.day1;

import java.util.Arrays;
import java.util.List;
import java.util.Map;
import java.util.stream.Collectors;

public class Nav {
	public static void main(String [] args) {
		List<String>l= Arrays.asList("male","female","male","female","male","female","male");
		Map<String, Long>aa= l.stream().collect(Collectors.groupingBy(r->r,Collectors.counting()));
		aa.forEach((x,y)-> System.out.println(x+"->"+y));
	}
}
