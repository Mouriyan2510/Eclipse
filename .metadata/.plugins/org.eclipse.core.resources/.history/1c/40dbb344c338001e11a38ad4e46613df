package com.one.day1;

public class Yogi {
    public static void main(String[] args) {
        int[] salaries = {1234, 789000000, 750000, 900000, 800000};
        int a=0;
        for(int i=0;i<salaries.length;i++) {
        	if(salaries[i]<a) {
        		a=salaries[i];
        	}
        }
        System.out.println(a);
    }
}
