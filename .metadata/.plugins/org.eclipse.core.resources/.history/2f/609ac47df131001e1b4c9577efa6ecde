package com.one.day1;

public class Count {
	public static void main(String[] args) {
		String a="teessshh";
		char ch[]=a.toCharArray();
		int count=0;
		for(int i=0;i<ch.length;i++) {
			for(int j=i+1;j<ch.length;j++) {
				count=0;
				if(ch[i]==ch[j]) {
					count=count+1;
					System.out.println(ch[i]+"--"+count);
				}
			}
		}
	}

}
