package com.one.day1;

import java.util.stream.LongStream;

public class Factorial {
	
	public static int factorial(int n) {
		if(n==0||n==1) {
			return 1;
		}
		else {
			return n* factorial(n-1);
		}
	}
	
	public static void main(String[] args) {
		int a=5;
		int r=factorial(a);
		System.out.println(r);
	}
		}



