package exper;

public class Dupli {
	public static void main(String[] args) {
		String word = "sucess";
		String temp = " ";
		char[] c = word.toCharArray();
		int count = 0;
		for (int i = 0; i < c.length; i++) {
			for (int j = i + 1; j < c.length; j++) {
				if (c[i] == c[j]) {
					temp = temp + c[i];
					count++;

				}
			}
		}
		System.out.print(temp);
		System.out.print("-"+count);
		
	}
}
