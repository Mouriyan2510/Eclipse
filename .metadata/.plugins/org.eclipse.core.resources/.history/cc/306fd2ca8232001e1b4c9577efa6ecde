package com.one.day1;

public class Pangram {
	public static void main(String[] args) {
		String s= "The quick brown fox jumps over the lazy dog";
		char[]ch=s.toCharArray();
		String a="";
		for(char i='a';i<='z';i++) {
			for(int j=0;j<ch.length;j++) {
				if(ch[j]==i) {
					a="Pangram";
				}
			}
		}
	}
	
}
