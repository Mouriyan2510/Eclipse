package TRYY;

import java.util.Arrays;

public class Sort {
	public static void main(String[] args) {
		int a[]= {3,6,8,1,7};
//		Arrays.sort(a);
//		for(Integer d:a) {
//		System.out.print(d);
//		}
		
		int b=0;
		for(int i=0;i<a.length;i++) {
			if(a[i]>b) {
				System.out.println(a[i]);
			}
		}
	}

}
