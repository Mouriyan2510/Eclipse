package com.one.day1;

public class Yogi {
    public static void main(String[] args) {
        double[] salaries = {1234, 7890, 750000, 900000, 800000};
        double a=0;
        for(int i=0;i<salaries.length;i++) {
        	if(salaries[i]>a) {
        		a=salaries[i];
        	}
        }
        System.out.println(a);
    }
}
