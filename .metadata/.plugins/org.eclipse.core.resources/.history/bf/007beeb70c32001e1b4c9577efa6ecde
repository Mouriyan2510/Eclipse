package com.one.day1;

import java.util.Arrays;
import java.util.List;

public class selva {
	public static void main(String[] args) {
		int[] a = { 521, 927,142};
		int a1=a[0];
		int a2=a[1];
		int a3=a[2];
		List<Integer>yu=Arrays.asList(a1,a2,a3);
		yu.stream().sorted().forEach(y->System.out.println(y));
//		System.out.println(Arrays.toString(a));
	}
}
