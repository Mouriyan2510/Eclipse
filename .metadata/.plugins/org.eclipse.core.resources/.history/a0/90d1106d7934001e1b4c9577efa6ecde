package exper;

import java.util.Scanner;


public class Prime {
	public static void main(String[] args) {
		int []a= {1,2,3,4,5,7,9,11,18};
		
		int i,j,count=0;
		
		for(i=0;i<a.length-1;i++) {
			count=0;
			if(a[i]>1) {
		
			for(j=2;j<a[i];j++) {
				if(a[i]%j==0) {
					count=1;
				}
			
			}
			}
			if(count>0) {
				System.out.println(a[i]+"   Not");
			}
			else {
				System.out.println(a[i]+"  Prime");
			}
		}
	}

}
