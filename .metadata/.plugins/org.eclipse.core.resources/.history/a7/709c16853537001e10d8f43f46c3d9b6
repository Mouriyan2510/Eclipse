package TRYY;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class Sort {
	public static void main(String[] args) {
		int a[]= {3,6,8,1,7};
//		Arrays.sort(a);
//		for(Integer d:a) {
//		System.out.print(d);
//		}
		
		List<int[]>aa=Arrays.asList(a);
		aa.stream().sorted().forEach(y-> {
			for(Integer d:a) {
				System.out.println(d);
			}
		});
	}

}
